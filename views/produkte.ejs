<script>

  document.addEventListener('DOMContentLoaded', () => {
    const menuBar = document.getElementById('menuBar');
    const menuArrow = document.getElementById('menuArrow');
    const mobileMenu = document.getElementById('navbar-menu');
  

    menuBar.addEventListener('click', () => {
      mobileMenu.classList.toggle('hidden');
      menuBar.classList.toggle('hidden');
      menuArrow.classList.toggle('hidden');
    });
  
    menuArrow.addEventListener('click', () => {
      mobileMenu.classList.toggle('hidden');
      menuBar.classList.toggle('hidden');
      menuArrow.classList.toggle('hidden');
    });

    document.addEventListener('click', (event) => {
      if (!mobileMenu.contains(event.target) && !menuBar.contains(event.target) && !menuArrow.contains(event.target)) {
        mobileMenu.classList.add('hidden');
        menuBar.classList.remove('hidden');
        menuArrow.classList.add('hidden');
      }
    });
  });
</script>


